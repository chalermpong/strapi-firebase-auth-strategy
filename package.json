{
  "name": "strapi-firebase-auth-strategy",
  "description": "Use Firebase Auth ID Token with your strapi backend",
  "version": "1.0.17",
  "keywords": [],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.23",
    "@strapi/icons": "^2.0.0-rc.23",
    "firebase-admin": "^13.3.0",
    "react-intl": "^7.1.11"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.12.7",
    "@strapi/typescript-utils": "^5.12.7",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.17",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.17"
  },
  "strapi": {
    "kind": "plugin",
    "name": "firebase-auth-strategy",
    "displayName": "Firebase Authentication Strategy - Strapi Plugin",
    "description": "Use Firebase Auth ID Token with your strapi backend"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chalermpong/strapi-firebase-auth-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/chalermpong/strapi-firebase-auth-plugin/issues"
  },
  "homepage": "https://github.com/chalermpong/strapi-firebase-auth-plugin#readme",
  "author": "chalermpong <s_pong_c@hotmail.com>"
}
